{
  "version": 2,
  "builds": [
    {
      "src": "api/main.js",
      "use": "@vercel/node",
      "config": {
        "functions": {
          "api/**/*.js": { "maxDuration": 60 }
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
    }
  ]
}
